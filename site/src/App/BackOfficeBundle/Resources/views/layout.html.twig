<!DOCTYPE html>
<html lang="en">
<head>
    <title>{% block title %}{{ setting.getData.title }} {% if app.request.attributes.get('_route_params')._breadcrumbs is defined %} - {{ (app.request.attributes.get('_route_params')['_breadcrumbs']|last)['label'] | trans }}{% endif %}
    {% endblock %}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Mauricio Ampuero">
    <link href="{{ asset('bundles/appfrontend/') }}imgs/favicon.png" rel="icon">
    <link href="{{ asset('bundles/appfrontend/') }}imgs/favicon.png" rel="apple-touch-icon">
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/select2/dist/css/select2.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/css/style.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/css/colors/default-dark.css') }}" id="theme" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/toast-master/css/jquery.toast.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/typeahead.js-master/dist/typehead-min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/css/custom.css') }}" id="theme" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/sweetalert/sweetalert.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/dropify/dist/css/dropify.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/Magnific-Popup-master/dist/magnific-popup.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/css/pages/user-card.css') }}" id="theme" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/plugins/jquery-ui/jquery-ui.css') }}" id="theme" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/plugins/jquery-ui/jquery-ui.css') }}" id="theme" rel="stylesheet">
    <link href="{{ asset('bundles/appbackoffice/css/pages/card-page.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/appbackoffice/css/pages/timeline-vertical-horizontal.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/wizard/steps.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/bootstrap-select/bootstrap-select.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/appbackoffice/assets/plugins/html5-editor/bootstrap-wysihtml5.css') }}" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" integrity="sha512-iBBXm8fW90+nuLcSKlbmrPcLa0OT92xO1BIsZ+ywDWZCvqsWgccV3gFoRBv0z+8dLJgyAHIhR35VZc2oM/gI1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/brands.min.css" integrity="sha512-apX8rFN/KxJW8rniQbkvzrshQ3KvyEH+4szT3Sno5svdr6E/CP0QE862yEeLBMUnCqLko8QaugGkzvWS7uNfFQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    {% block stylesheet %}
    {% endblock %}
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/jquery/jquery.min.js') }}"></script>
</head>
<body class="fix-header card-no-border fix-sidebar">
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label">Cargando, por favor espere...</p>
        </div>
    </div>
    <div id="main-wrapper">
        <header class="topbar">
            {{ include('AppBackOfficeBundle::header.html.twig') }}
        </header>
        {{ include('AppBackOfficeBundle::leftMenu.html.twig') }}
        <div class="page-wrapper">
            <div class="container-fluid">
                {% if cardContainer is defined and cardContainer == true %}
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-body">
                                {% endif %}
                                    {% block body %}
                                    {% endblock %}
                                {% if cardContainer is defined and cardContainer == true %}
                                </div>
                            </div>
                        </div>
                    </div>
                {% endif %}
            </div>
            <footer class="footer">
                {{ setting.getData.copyright }}
            </footer>
        </div>
    </div>
    {% block javascripts %}
    
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/bootstrap/js/popper.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/js/perfect-scrollbar.jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/js/waves.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/js/sidebarmenu.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/sticky-kit-master/dist/sticky-kit.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/typeahead.js-master/dist/typeahead.bundle.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/js/custom.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/js/handlebars.min.js') }}"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.form/4.2.2/jquery.form.min.js"></script> 
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/sweetalert/sweetalert.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/toast-master/js/jquery.toast.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/Magnific-Popup-master/dist/jquery.magnific-popup.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/Magnific-Popup-master/dist/jquery.magnific-popup-init.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/js/sendAjax.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/bootstrap-select/bootstrap-select.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/select2/dist/js/select2.full.min.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/html5-editor/wysihtml5-0.3.0.js') }}"></script>
    <script src="{{ asset('bundles/appbackoffice/assets/plugins/html5-editor/bootstrap-wysihtml5.js') }}"></script>
    <script>
    
    var loading={
        show: function (text){
            if(typeof text !== "undefined") $('#loading').attr('data-text',text);
            {# $('#loading').show(); #}
            $(".preloader").fadeIn();
            return true;
        },
        hide: function (){
            {# $('#loading').removeAttr('data-text');
            $('#loading').hide(); #}
            $(".preloader").fadeOut();
            return true;
        }
    }
    
    $(function (){
        $('.selectpicker').selectpicker();
        $('.textarea_editor').wysihtml5();
        $(".select2").select2();
        if(typeof $.fn.datepicker.dates != 'undefined'){
            $.fn.datepicker.dates['es'] = {
                days: ['Domingo', 'Lunes', 'Martes', 'Miércoles', 'Jueves', 'Viernes', 'Sábado'],
                daysShort:['Dom','Lun','Mar','Mié','Juv','Vie','Sáb'],
                daysMin: ['Do','Lu','Ma','Mi','Ju','Vi','Sá'],
                months: ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre', 'Diciembre'],
                monthsShort:  ['Ene','Feb','Mar','Abr', 'May','Jun','Jul','Ago','Sep', 'Oct','Nov','Dic'],
                today: "Hoy",
                clear: "Limpiar",
                format: "dd/mm/yyyy",
                titleFormat: "MM yyyy",
                weekStart: 0
            };
            $.fn.datepicker.defaults.language = 'es';
        }
        
        jQuery('.datepicker').datepicker({
            autoclose: true,
            language: 'es',
            format: "dd/mm/yyyy",
            todayHighlight: true
        });

        $(".sidebartoggler").on('click', function() {
            console.log("dsdsa");
            if ($("body").hasClass("mini-sidebar")) {
                $("body").trigger("resize");
                $("body").removeClass("mini-sidebar");
                $('.navbar-brand span').show();
                var menuDisplay="large";
            } else {
                $("body").trigger("resize");
                $("body").addClass("mini-sidebar");
                $('.navbar-brand span').hide();
                var menuDisplay="small";
            }
            {# $.ajax({
                url:"{{ path('api_users_put_attribute',{ id: app.user.id })}}",
                type: 'PUT',
                data:JSON.stringify({ menuDisplay: menuDisplay })
            }); #}
        });

        /* Seteo el menu segun la variable del usaurio */
       
        {# {% if app.user.menuDisplay == 'small' %}
            setTimeout(function(){ 
                $("body").trigger("resize");
                $("body").addClass("mini-sidebar");
                $('.navbar-brand span').hide();
            }, 0);
        {% endif %} #}

        
        {% for message in app.session.flashBag.get('success') %}
        $.toast({
            heading: '{{ 'SUCCESS' | trans }}',
            text: '{{ message }}',
            position: 'top-right',
            icon: 'success',
            hideAfter: 3000, 
            stack: 6
        });
        {% endfor %}
        {% for message in app.session.flashBag.get('warning') %}
        $.toast({
            heading: '{{ 'WARNING' | trans }}',
            text: '{{ message }}',
            position: 'top-right',
            icon: 'warning',
            hideAfter: 10000, 
            stack: 6
        });
        {% endfor %}
        {% for message in app.session.flashBag.get('danger') %}
        $.toast({
            heading: '{{ 'ERROR' | trans }}',
            text: '{{ message }}',
            position: 'top-right',
            icon: 'error',
            hideAfter: 6000, 
            stack: 6
        });
        {% endfor %}
    });
    </script>
    {% endblock %}
</body>
</html>
